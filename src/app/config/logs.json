{
  "version": 1,
  "formatters": {
    "default": {
      "format": "[%(asctime)s] [%(levelname)s] - [%(module)s] %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default"
    },
    "file": {
      "class": "logging.FileHandler",
      "filename": "liturgical-api.log",
      "formatter": "default"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["console"]
  }
}